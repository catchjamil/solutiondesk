<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.sd.web.model.bo.TechnologyBO" dynamic-insert = "true" dynamic-update = "true" table="TECHNOLOGY">
<id name="id" column="ID">
            <generator class="increment"/>
        </id>
        
        <property name="techName" column="TECH_NAME"/>
        <property name="chapterName" column="CHAPTER_NAME"/>
        <property name="topicName" column="TOPIC_NAME"/>
        
        <property name="action" column="ACTION"/>
        <property name="createdBy" column="CREATEDBY"/>
        <property name="createdOn" column="CREATEDON"/>
        <property name="modifiedBy" column="MODIFIEDBY"/>
        <property name="modifiedOn" column="MODIFIEDON"/>
        
        
    </class>

</hibernate-mapping>